                  <h3><%= fullname%> (<%= qstatus%>)</h3>
                </div>
              </div>
              <div class="form-text">Please wait for your queue number to be called/displayed on the monitor.</div>
            </div>
            <div class="w3-container w3-border-top w3-padding-16 text-end">
              <button type="button" class="btn btn-primary" id="download"><i class="fas fa-print"></i> Download</button>
              <button type="button" class="btn btn-secondary" onclick="document.getElementById('successQueueRegistar').style.display='none'">Close</button>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!--end Success Queue Registrar Modal-->
    
    <!-- Footer -->
    <footer class="w3-center w3-padding-16" style="background-color: #116530; color: white">
      <div class="credit"> Copyright © 2022 - <span>E-Queuing Management System</span> | Acob, Acoba, Bagaoisan, and Ordonia </div>
    </footer>
      
      <script>
               
        var ans = '<%- ans %>'
        if(ans == 'sQueueCashier'){
          w3.show('#successQueueCashier')
        } else if(ans == 'sQueueDean'){
          w3.show('#successQueueDean')
        } else if(ans == 'sQueueDepChair'){
          w3.show('#successQueueDepChairman')
        } else if(ans == 'sQueueGuidance'){
          w3.show('#successQueueGuidance')
        } else if(ans == 'sQueueRegistrar'){
          w3.show('#successQueueRegistar')
        }

        //download pdf///
        var download = document.getElementById("download");
        var makepdf = document.getElementById("makepdf");

        download.addEventListener("click", function () {
            html2pdf().set({margin: [0,1,1,1], filename: '<%= fullname%> QueueNumber.pdf', html2canvas:  { scale: 2 }, jsPDF: { unit: 'in', format: 'A4', orientation: 'portrait' }}).from(makepdf).save();
        });

        // Toggle between showing and hiding the sidebar when clicking the menu icon
        var mySidebar = document.getElementById("mySidebar");

        function w3_open() {
          if (mySidebar.style.display === 'block') {
            mySidebar.style.display = 'none';
          } else {
            mySidebar.style.display = 'block';
          }
        }

        // Close the sidebar with the close button
        function w3_close() {
            mySidebar.style.display = "none";
        }
      </script>
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>
  </body>
</html>