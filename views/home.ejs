          <h4 class="modal-title">Cashier Queue Form</h4>
          <button type="button" class="btn-close" onclick="document.getElementById('queuemodalCashier').style.display='none'"></button>
        </div>
      </div>
      <form method="POST" class="w3-container w3-padding-16">
        <div class="w3-container">
          <p class="w3-row w3-left w3-padding-small" id="displayDATE"></p>
          <p class="w3-row w3-left w3-padding-small" id="displayTIME"></p>
          <input type="hidden" id="timeTodatabase" name="times" value="time">
          <input type="hidden" id="dateTodatabase" name="dates" value="date">  
        </div>
        <div class="form-floating mb-3">
          <input name="queuename" type="text" class="form-control" placeholder="Full Name" required>
          <label for="queuename">Full name</label>
        </div>
        <div class="form-floating mb-3">
          <select name="queuestatus" class="form-select" aria-label="Floating label" required>
            <option value="">Select status</option>
            <option value="Student">Student</option>
            <option value="Guest">Guest</option>
            <option value="Senior/Pwd">Senior/Pwd</option>
          </select>
          <label for="queuestatus">Status</label>
        </div>
        <input class="w3-button w3-block w3-green w3-section w3-padding w3-round" type="submit" formaction="/queue_cashier" value="Submit">
      </form>
    </div>
  </div>
